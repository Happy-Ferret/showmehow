#!/usr/bin/make -f

# This file was automatically generated by stdeb 0.8.5 at
# Thu, 04 Aug 2016 08:14:40 +0800

%:
	dh $@ --with python2,python3 --buildsystem=python_distutils


override_dh_auto_clean:
	python setup.py clean -a
	python3 setup.py clean -a
	find . -name \*.pyc -exec rm {} \;



override_dh_auto_build:
	python setup.py build --force
	python3 setup.py build --force



override_dh_auto_install:
	python setup.py install --force --root=debian/python-showmehow --no-compile -O0 --install-layout=deb --prefix=/usr 
	cp lessons.json debian/python-showmehow/usr/lib/python2.7/dist-packages/showmehow/
	python3 setup.py install --force --root=debian/python3-showmehow --no-compile -O0 --install-layout=deb --prefix=/usr 
	cp lessons.json debian/python3-showmehow/usr/lib/python3/dist-packages/showmehow/



override_dh_python2:
	dh_python2 --no-guessing-versions




